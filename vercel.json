{
  "version": 2,
  "builds": [{"src": "**/*", "use": "@vercel/static"}],
  "routes": [
    {"src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|mp3|json))", "dest": "/$1"},
    {"src": "/public/(.*)", "dest": "/public/$1"},
    {"src": "/game", "dest": "/game.html"},
    {"src": "/(.*)", "dest": "/index.html"}
  ]
}
